function sendAjax(ajax) {
    if(ajax.type==="POST"){
        return axios({
            url:ajax.url,
            method:ajax.type,
            data:JSON.stringify(ajax.data()),
            headers: {
                "Content-Type": "application/json; charset=utf-8;"
            }
        })
    }else{
        return axios({
            url:ajax.url,
            method:ajax.type,
            params:ajax.data(),
            headers: {
                "Content-Type": "application/json; charset=utf-8;"
            }
        })
    }
}

